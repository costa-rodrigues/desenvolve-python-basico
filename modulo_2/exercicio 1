      1 # preencha o print com sua resposta
----> 2 soma = n1 + n2
      3 print(soma)
      4 print("o script vai dar erro pois não foi salvo valores nas variáveis n1 e n2 ")

NameError: name 'n1' is not defined
# Script 3
a = 10
a = a + 1
print(a)
# preencha o print com sua resposta
a = 10
a = a + 1
print( a )
print(" o script esta correto ")
11
 
# Script 4
saldo da conta = 1500
0.1 = juros
print(saldo da conta * juros)
# preencha o print com sua resposta
saldo da conta = 1500
0.1 = juros
print(saldo da conta * juros)
print(" há um erro: o nome da variável 'juros' esta após o valor da variável ")
